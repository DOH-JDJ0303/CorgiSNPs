      "spec": "{\n  \"$schema\": \"https://vega.github.io/schema/vega-lite/v5.json\",\n  \"autosize\": {\n    \"type\": \"fit-x\",\n    \"resize\": true,\n    \"contains\": \"padding\"\n  },\n  \"width\": \"container\",\n  \"height\": 300,\n  \"config\": {\n    \"axis\": {\n      \"labelFontSize\": 12,\n      \"titleFontSize\": 14,\n      \"titlePadding\": 10,\n      \"grid\": true,\n      \"gridColor\": \"#e0e0e0\",\n      \"gridOpacity\": 0.5,\n      \"domain\": false,\n      \"tickColor\": \"#666666\"\n    },\n    \"legend\": {\n      \"labelFontSize\": 12,\n      \"titleFontSize\": 14,\n      \"symbolSize\": 100\n    },\n    \"header\": {\n      \"labelFontSize\": 12,\n      \"titleFontSize\": 14\n    },\n    \"title\": {\n      \"fontSize\": 16,\n      \"fontWeight\": \"bold\",\n      \"anchor\": \"start\",\n      \"offset\": 10\n    },\n    \"view\": {\n      \"stroke\": \"transparent\"\n    },\n    \"background\": \"#fafafa\"\n  },\n  \"data\": {\n    \"name\": \"table\"\n  },\n  \"transform\": [\n    {\n      \"calculate\": \"toNumber(datum.core_fraction)\",\n      \"as\": \"core_fraction_num\"\n    },\n    {\n      \"calculate\": \"toNumber(datum.genome_fraction)\",\n      \"as\": \"genome_fraction_num\"\n    },\n    {\n      \"window\": [\n        {\n          \"op\": \"row_number\",\n          \"as\": \"order\"\n        }\n      ],\n      \"sort\": [\n        {\n          \"field\": \"genome_fraction_num\",\n          \"order\": \"descending\"\n        }\n      ]\n    }\n  ],\n  \"mark\": {\n    \"type\": \"line\",\n    \"point\": {\n      \"size\": 80,\n      \"filled\": true,\n      \"strokeWidth\": 1.5,\n      \"stroke\": \"white\"\n    },\n    \"strokeWidth\": 2.5,\n    \"color\": \"#2E86AB\",\n    \"interpolate\": \"monotone\"\n  },\n  \"encoding\": {\n    \"x\": {\n      \"field\": \"sample\",\n      \"type\": \"nominal\",\n      \"sort\": {\n        \"field\": \"genome_fraction_num\",\n        \"op\": \"mean\",\n        \"order\": \"descending\"\n 